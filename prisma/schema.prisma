generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String? @unique
  passwordHash String?
  files File[]
}

model File {
  id  Int @id @default(autoincrement())
  filename  String
  size  Int?
  url String?
  uplaodedAt  DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  user  User  @relation(fields: [userId], references: [id])
  userId  Int
}